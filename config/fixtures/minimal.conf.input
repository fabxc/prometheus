global <
  scrape_interval: "30s"
  evaluation_interval: "30s"
  labels: <
    label: <
      name: "monitor"
      value: "test"
    >
  >
  rule_file: "prometheus.rules"
>

job: <
  name: "prometheus"
  scrape_interval: "15s"
  metrics_path: "/metrics"
  scheme: "http"

  target_group: <
    target: "localhost:9090"
  >
>
